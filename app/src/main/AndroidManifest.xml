<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.rudimentpractice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SingleStrokeRudActivity"
            android:label="@string/category_singles">
        </activity>
        <activity
            android:name=".DoubleStrokeRudActivity"
            android:label="@string/category_doubles">

        </activity>
        <activity
            android:name=".MultipleRollRudActivity"
            android:label="@string/category_bounce">

        </activity>
        <activity
            android:name=".DiddleStrokeRudActivity"
            android:label="@string/category_diddle">

        </activity>
        <activity
            android:name=".FlamRudActivity"
            android:label="@string/category_flam">

        </activity>
        <activity
            android:name=".DragRudActivity"
            android:label="@string/category_drag">

        </activity>
    </application>

</manifest>